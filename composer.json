{
    "name": "platformcommunity/flysystem-bunnycdn",
    "description": "Flysystem adapter for BunnyCDN",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Sinnott",
            "email": "alex@platformapp.io"
        }
    ],
    "require": {
        "league/flysystem": "^2.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.4",
        "league/mime-type-detection": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "mockery/mockery": "^1.3",
        "league/flysystem-adapter-test-utilities": "^2",
        "league/flysystem-memory": "^2.0",
        "fzaninotto/faker": "^1.5"
    },
    "autoload-dev": {
        "psr-4": {
            "PlatformCommunity\\Flysystem\\BunnyCDN\\Tests\\": "tests/"
        }
    },
    "autoload": {
        "psr-4": {
            "PlatformCommunity\\Flysystem\\BunnyCDN\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
