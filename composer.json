{
    "name": "platformcommunity/flysystem-bunnycdn",
    "description": "Flysystem adapter for BunnyCDN",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Sinnott",
            "email": "alex@platformapp.io"
        }
    ],
    "repositories": {
        "bunnycdn-storage": {
            "type": "package",
            "package": {
                "name": "bunnyway/bunnycdn.php.storage",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/BunnyWay/BunnyCDN.PHP.Storage",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["/"]
                }
            }
        }
    },
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "~1.0",
        "guzzlehttp/guzzle": "~6.0",
        "bunnyway/bunnycdn.php.storage": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "PlatformCommunity\\Flysystem\\BunnyCDN\\": "src/"
        },
        "classmap": [
            "vendor/bunnyway/bunnycdn.php.storage/bunnycdn-storage.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
