{
    "name": "platformcommunity/flysystem-bunnycdn",
    "description": "Flysystem adapter for BunnyCDN",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Sinnott",
            "email": "alex@platformapp.io"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "league/flysystem": "~1.0",
        "guzzlehttp/guzzle": "~6.0",
        "platformcommunity/bunnycdn-storage": "dev-master",
        "pcov/clobber": "^2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
