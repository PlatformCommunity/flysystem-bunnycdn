        }
    },
    "repositories": [
        { "name": "platformcommunity/bunnycdn-storage", "type": "vcs", "url": "https://github.com/PlatformCommunity/BunnyCDN.PHP.Storage.git" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
